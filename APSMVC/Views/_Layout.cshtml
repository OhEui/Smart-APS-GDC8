<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/pages/plugins/fontawesome-free/css/all.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="~/pages/plugins/select2/css/select2.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/pages/dist/css/adminlte.min.css">
    <link rel="stylesheet" href="~/pages/amCharts/css/default.css">
    <!-- daterange picker -->
    <link rel="stylesheet" href="~/pages/plugins/daterangepicker/daterangepicker.css">

</head>
<body class="hold-transition sidebar-mini">

    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>

        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="#" class="brand-link">
                <img src="~/pages/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">Smart APS</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/pages/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                        <!--<i class="fas fa-user-circle" style="color:white;font-size:25px;"></i>-->

                    </div>
                    <div class="info">
                        <a href="/User/Login" class="d-block">Login Test</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->
                        <li class="nav-header">RESULT</li>
                        <li class="nav-item">
                            <a href="/result/LOTgantt" class="nav-link">
                                <i class="nav-icon far fa-calendar-alt"></i>
                                <p>
                                    LOT Gantt
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/result/EQPgantt" id="LoadEqpGantt" class="nav-link">
                                <i class="nav-icon far fa-image"></i>
                                <p>
                                    EQP Gantt
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/result/utilization" class="nav-link">
                                <i class="nav-icon fas fa-columns"></i>
                                <p>
                                    Utilization
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper pt-3">
            @RenderBody()
        </div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/pages/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/pages/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/pages/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/pages/dist/js/demo.js"></script>
    <!-- Select2 -->
    <script src="~/pages/plugins/select2/js/select2.full.min.js"></script>

    <!-- amCharts -->
    <script src="https://cdn.amcharts.com/lib/5/index.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/xy.js"></script>
    <script src="https://cdn.amcharts.com/lib/5/themes/Animated.js"></script>

    <!-- date-range-picker -->
    <script src="~/pages/plugins/moment/moment.min.js"></script>
    <script src="~/pages/plugins/daterangepicker/daterangepicker.js"></script>

    <script>
        $(() => {
            /*
            $('#LoadEqpGantt').on("click", () => {
                $.ajaxSetup['before']
                $.ajax({
                    type: 'GET',
                    url: 'https://localhost:44397/Result/EqpGantt'
                }).done(function (data) {
                    $('#').html(data);
                }).fail(function (jaxhr, status) {
                    alert('fail');
                    alert(jaxhr.status);
                });
            });
            */
        });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
